    <table class="table table-striped">
    <thead>
      <tr>
        <th data-toggle="tooltip" data-placement="top" title="just the tip">Player</th>
        <th data-toggle="tooltip" data-placement="top" title="just the tip">Team</th>
        <th data-toggle="tooltip" data-placement="top" title="just the tip">Pos</th>
        <th data-toggle="tooltip" data-placement="top" title="just the tip">Gp</th>
        <th data-toggle="tooltip" data-placement="top" title="just the tip">G</th>
        <th data-toggle="tooltip" data-placement="top" title="just the tip">A</th>
        <th data-toggle="tooltip" data-placement="top" title="just the tip">P</th>
        <th data-toggle="tooltip" data-placement="top" title="just the tip">penaltiesPerSixty</th>
        <th data-toggle="tooltip" data-placement="top" title="just the tip">pointsPerSixty</th>
        <th data-toggle="tooltip" data-placement="top" title="just the tip">FsPoints</th>
        <th data-toggle="tooltip" data-placement="top" title="just the tip">X</th>
      </tr>
    </thead>
    <tbody>
     {{#each items.data}}
      <tr>
       <td>{{playerName}}</td>
        <td class="small-logo {{helperLowerCase playerTeamsPlayedFor}}"></td>
        <td>{{playerPositionCode}}</td>
        <td>{{gamesPlayed}}</td>
        <td>{{goals}}</td>
        <td>{{assists}}</td>
        <td>{{points}}</td>
        <td>{{penaltiesPerSixty}}</td>
        <td>{{pointsPerSixty}}</td>
        <td>{{fsPoints}}</td>
        <td>dasdadss</td>
      </tr>
      {{/each}}
    </tbody>
  </table>